        <activity
            android:name="com.eaosoft.mclerk.GWareHouseStatistics"
            android:label="@string/title_activity_gware_house_statistics" />
        <activity
            android:name="com.eaosoft.mclerk.GWareHouseStatisticsDetail"
            android:label="@string/title_activity_gware_house_statistics_detail" />

        <service android:name="net.posprinter.service.PosprinterService" />

        <activity android:name="com.eaosoft.mclerk.GCashier_Package" >
        </activity>
        <activity android:name="com.eaosoft.mclerk.GCashier_Package_New" >
        </activity>
        <activity android:name="com.eaosoft.mclerk.GCashier_Search" >
        </activity>
        <activity android:name="com.eaosoft.mclerk.CardSalse" >
        </activity>
        <activity android:name="com.eaosoft.mclerk.CardSearch" >
        </activity>
        <activity android:name="com.eaosoft.mclerk.Consumption_summary" >
        </activity>
        <activity android:name="com.eaosoft.mclerk.PerformanceRanking" >
        </activity>
        <activity android:name="com.eaosoft.mclerk.SalesPersonal" >
        </activity>
        <activity android:name="com.eaosoft.mclerk.DifferentStoreSales" >
        </activity>
    </application>

</manifest>